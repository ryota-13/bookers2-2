<%= render 'books/users', book: @book, user: @user %>

<h1>Users</h1>
<% @users.each do |user| %>
  <p>
  image<%= image_tag user.get_profile_image(100,100) %> 
  </p>
  <p>
  name<%= user.name %>
  </p>
  <% if @user == current_user %>
     <p><%= link_to "show", user_path(@user) %></p>
  <% end %> 
<% end %>
